{
    "title": "算法之路",
    "description": "算法教程与源码",
    "language": "zh-hans",
    "author": "林荣彬",
    "plugins": [
        "mathjax@https://github.com/OrgVue/gitbook-plugin-mathjax.git#speech-fix",
        "splitter",
        "-search",
        "-lunr",
        "livereload",
        "autocover"
    ],
    "pluginsConfig": {
        "search": {
            "maxIndexSize": 100000
        },
        "include-codeblock": {
            "template": "ace",
            "theme": "monokai"
        },
        "autocover": {
            "font": {
                "size": null,
                "family": "Impact",
                "color": "#FFF"
            },
            "size": {
                "w": 1800,
                "h": 2360
            },
            "background": {
                "color": "#09F"
            }
        }
    }
}
